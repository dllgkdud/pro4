<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample">
	<select id="memberList" resultType="com.samsung.dto.MemberDTO">
		select * from member
	</select>
	<insert id="memberJoin">
		insert into member(id, pw, name, tel, addr) values #{id}, #{pw}, #{name}, #{tel}, #{addr}
	</insert>
</mapper>
<!-- ArrayList<MemberDTO> memList = sample.memberList(); 시 해당 실행 리스트를 리스트로 저장해준다. -->
MemberDTO member = new MemberDTO();
member.setId(id);
member.setPw(pw);
member.setName(name);

<!-- sample.memberJoin(member); 자동으로 dto에 저장하고, controller에서 사용하도록 지정한다. -->
<!-- namespace나 해당 명령의 id는 절대 중복될 수 없음 -->